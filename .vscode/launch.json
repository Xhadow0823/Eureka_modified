{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "(FrankaCubeStack)Run Eureka",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/eureka/",
            "program": "eureka.py",
            "args": [
                "model=gpt-4-turbo-preview",
                "env=franka_cube_stack",
                "iteration=10",  // 1
                "sample=1",  // 3
                "max_iterations=1000"  // HOW TO SET THE TRAINING EPOCH ???
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "Run LLMRLT",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/LLMRLT/",
            "program": "main.py",
            "args": [
                "model=gpt-4-turbo-preview",
                "env=franka_cube_stack",
                "iteration=10",  // 1
                "sample=1",  // 3
                "max_iterations=1000"  // HOW TO SET THE TRAINING EPOCH ???
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}