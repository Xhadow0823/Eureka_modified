{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        
        {
            "name": "(FrankaCubeStack)Run Eureka",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/eureka/",
            "program": "eureka.py",
            "args": [
                "model=gpt-4-turbo-preview",
                "env=franka_cube_stack",
                "iteration=10",  // 1
                "sample=1",  // 3
                "max_iterations=1000"  // HOW TO SET THE TRAINING EPOCH ???
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },

        {
            "name": "Load Checkpoint",
            "type": "debugpy",
            "request": "launch",
            "program": "./isaacgymenvs/isaacgymenvs/train.py",
            "args": [
                // "task=FrankaCubeStackRRR",
                "task=FrankaCabinetRRR",
                "num_envs=2",
                "train.params.config.minibatch_size=4",
                
                "test=True",
                "task.env.enableDebugVis=True",
                "force_render=True",
                "headless=False",
                // "train.params.config.max_epochs=1000",
                
                // "checkpoint='policy-2024-06-28_20-49-23/runs/FrankaCubeStackRRR-2024-06-28_20-49-24/nn/last_FrankaCubeStackRRR_ep_1000.pth'",
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        },
    ]
}